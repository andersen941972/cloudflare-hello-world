{
    "name": "Cloudflare Workers Hello World",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "forwardPorts": [
        8787
    ],
    "postCreateCommand": "npm install",
    "remoteUser": "node",
    "customizations": {
        "vscode": {
            "extensions": [
                "cloudflare.vscode-wrangler",
                "dbaeumer.vscode-eslint"
            ]
        }
    }
}